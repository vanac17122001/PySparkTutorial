    "from pyspark import SparkContext"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "sc=SparkContext.getOrCreate()\n",
    "x = sc.parallelize([(\"spark\",1),(\"hadoop\",2)])\n",
    "y = sc.parallelize([(\"spark\",2),(\"hadoop\",5)])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Join RDD -> [('hadoop', (2, 5)), ('spark', (1, 2))]\n"
     ]
    }
   ],
   "source": [
    "joined = x.join(y)\n",
    "final = joined.collect()\n",
    "print(\"Join RDD -> %s\" % (final))"
   ]
  }
 ],
